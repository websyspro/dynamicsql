Como Usar:

<?php

class Documet
{
  public int $Id;
  public string $Description;
}

$findByFN = new TFindByFN(
  fn(Document $d) => (
    $d->Id === 1 &&
    $d->Description === "Compras%"
  )
);

var_dump($findByFN->getConditions());

Result:: "Document.Id = 1 And Document.Description Like 'Compras%'"